      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>select 
service_provider_id,
service_providers_name as service_provider_name,
lessees.name as lessee_name,
contracts.place_no as place,
contracts.place_type_id,
contracts.lessee_id,
place_types.name as place_type_name,
ifnull(sum_1, 0) as sum_1, ifnull(paidsum_1, 0) as paidsum_1, ifnull(count_1, 0) as count_1, ifnull(sum_2, 0) as sum_2, ifnull(paidsum_2, 0) as paidsum_2, ifnull(count_2, 0) as count_2, ifnull(sum_3, 0) as sum_3, ifnull(paidsum_3, 0) as paidsum_3, ifnull(count_3, 0) as count_3,
@month_1 as month_1,
@month_2 as month_2,
@month_3 as month_3
,price_1
,price_2
,price_3
from (
select accrual.contract_id, services.service_provider_id, service_providers.name as service_providers_name
FROM accrual
LEFT JOIN accrual_pays ON accrual_pays.accrual_id = accrual.id
LEFT JOIN services ON accrual_pays.service_id = services.id
LEFT JOIN service_providers on service_providers.id = services.service_provider_id 
WHERE year = @year AND (accrual.month = @month_1 OR accrual.month = @month_2 OR accrual.month = @month_3) 
    AND (-1 IN (@providers) OR service_providers.id IN (@providers))
GROUP BY accrual.contract_id) as t0
    
LEFT JOIN contracts ON contracts.id = t0.contract_id
LEFT JOIN lessees ON contracts.lessee_id = lessees.id
LEFT JOIN place_types ON place_types.id = contracts.place_type_id
    
LEFT JOIN (SELECT accrual.contract_id, accrual.id as id, month, year, no_complete, sumtable.sum as sum_1, paidtable.sum as paidsum_1 
, sumtable.count as count_1, sumtable.price as price_1
FROM accrual 
LEFT JOIN (SELECT accrual_id, count, price, SUM(count * price) as sum
			FROM accrual_pays 
			LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
            where (-1 IN (@providers) OR service_providers.id IN (@providers))
			GROUP BY accrual_id ) as sumtable ON sumtable.accrual_id = accrual.id 
LEFT JOIN (SELECT accrual_id, SUM(sum) as sum FROM accrual_pays 
			LEFT JOIN payment_details ON accrual_pays.id=payment_details.accrual_pay_id 
            LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
			where (-1 IN (@providers) OR service_providers.id IN (@providers))
            GROUP BY accrual_id) as paidtable ON paidtable.accrual_id = accrual.id 
WHERE year = @year AND accrual.month = @month_1
																		) t1 on t1.contract_id = t0.contract_id

LEFT join (SELECT accrual_2.contract_id, no_complete, sumtable_2.sum as sum_2, paidtable_2.sum as paidsum_2 
, sumtable_2.count as count_2, sumtable_2.price as price_2
FROM accrual as accrual_2
LEFT JOIN (SELECT accrual_id, count, price, SUM(count * price) as sum 
			FROM accrual_pays 
			LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
            where (-1 IN (@providers) OR service_providers.id IN (@providers))
			GROUP BY accrual_id) as sumtable_2 ON sumtable_2.accrual_id = accrual_2.id 
LEFT JOIN (SELECT accrual_id, SUM(sum) as sum
			FROM accrual_pays 
			LEFT JOIN payment_details ON accrual_pays.id=payment_details.accrual_pay_id 
            LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
			where (-1 IN (@providers) OR service_providers.id IN (@providers))
            GROUP BY accrual_id) as paidtable_2 ON paidtable_2.accrual_id = accrual_2.id 
WHERE accrual_2.year = @year AND accrual_2.month = @month_2
												) as t2 on t2.contract_id = t0.contract_id

LEFT join (SELECT accrual.contract_id, accrual.id as id, month, year, no_complete, sumtable.sum as sum_3, 
paidtable.sum as paidsum_3 
, sumtable.count as count_3, sumtable.price as price_3
FROM accrual 
LEFT JOIN (SELECT accrual_id, count, price, SUM(count * price) as sum 
			FROM accrual_pays 
			LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
            where (-1 IN (@providers) OR service_providers.id IN (@providers)) 
			GROUP BY accrual_id) as sumtable ON sumtable.accrual_id = accrual.id 
LEFT JOIN (SELECT accrual_id, SUM(sum) as sum FROM accrual_pays 
			LEFT JOIN payment_details ON accrual_pays.id=payment_details.accrual_pay_id 
            LEFT JOIN services ON accrual_pays.service_id = services.id
            LEFT JOIN service_providers on service_providers.id = services.service_provider_id
			where (-1 IN (@providers) OR service_providers.id IN (@providers))
            GROUP BY accrual_id) as paidtable ON paidtable.accrual_id = accrual.id 
WHERE year = @year AND accrual.month = @month_3
          ) as t3 on t3.contract_id = t0.contract_id
WHERE
(@onlyAccruals = 0 OR (@onlyAccruals = 1 AND 
count_1 is not null
AND count_2 is not null
AND count_3 is not null))
order by lessees.name, place_types.name, contracts.place_no;</CommandText>
        <QueryParameters>
          <QueryParameter Name="providers">
            <Value>=Parameters!providers</Value>
          </QueryParameter>
          <QueryParameter Name="month_1">
            <Value>=Parameters!month_1</Value>
          </QueryParameter>
