      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT reading.meter_month_1, reading.meter_month_2, reading.meter_month_3, service_providers.name as service_provider_name, lessees.name,  place_types.name as place_type, meters.place_no, 
meter_types.name type, 
  IFNULL(accrual_pays_1.count, 0) as count_1, IFNULL(accrual_pays_2.count, 0) as count_2, IFNULL(accrual_pays_3.count, 0) as count_3,  IFNULL(paysum_1.sum, 0) as paid_1, IFNULL(paysum_2.sum, 0) as paid_2, IFNULL(paysum_3.sum, 0) as paid_3, IFNULL(reading.lastpayid_1, -1) as lastpayid0_1,
 IFNULL(reading.lastpayid_2, -1) as lastpayid0_2, IFNULL(reading.lastpayid_3, -1) as lastpayid0_3, 
IFNULL(accrual_pays_1.price,0) as price_1, IFNULL(accrual_pays_2.price, 0) as price_2, 
 IFNULL(accrual_pays_3.price, 0) as price_3
 FROM meters 
LEFT JOIN (
SELECT  selectedmeters.*, 
(SELECT accrual_pay_id 
	FROM meter_reading 
    WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = (SELECT * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 1)# for 1 month 
    AND YEAR(meter_reading.date) = @year 
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastpayid_1,
    (SELECT  * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 1) meter_month_1,
    
    (SELECT accrual_pay_id 
	FROM meter_reading 
    WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = (SELECT * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 1, 1)# for 2 month 
    AND YEAR(meter_reading.date) = @year 
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastpayid_2,
    (SELECT  * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 1, 1) meter_month_2,
    
        (SELECT accrual_pay_id 
	FROM meter_reading 
    WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = (SELECT * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 2, 1)# for 2 month 
    AND YEAR(meter_reading.date) = @year 
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastpayid_3,
    (SELECT  * FROM
        (SELECT 1 AS mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
    WHERE t.mon IN (@month) LIMIT 2, 1) meter_month_3
    
FROM ( SELECT meters.id as meter_id,meter_tariffs.id as meter_tariff_id FROM meters JOIN meter_tariffs on meters.meter_type_id=meter_tariffs.meter_type_id

) as selectedmeters
) as reading ON meters.id = reading.meter_id 
LEFT JOIN place_types ON place_types.id = meters.place_type_id
LEFT JOIN meter_types ON meters.meter_type_id = meter_types.id
LEFT JOIN meter_tariffs ON meter_tariffs.id = reading.meter_tariff_id
LEFT JOIN active_contracts ON meters.place_type_id = active_contracts.place_type_id AND meters.place_no = active_contracts.place_no
LEFT JOIN lessees ON active_contracts.lessee_id = lessees.id
LEFT JOIN accrual_pays as accrual_pays_1 ON accrual_pays_1.id = reading.lastpayid_1 
LEFT JOIN accrual_pays as accrual_pays_2 ON accrual_pays_2.id = reading.lastpayid_2 
LEFT JOIN accrual_pays as accrual_pays_3 ON accrual_pays_3.id = reading.lastpayid_3 
LEFT JOIN services on services.id = accrual_pays_1.service_id
LEFT JOIN service_providers on service_providers.id = services.service_provider_id
LEFT JOIN (SELECT accrual_pay_id, SUM(sum) as sum FROM payment_details GROUP BY accrual_pay_id) as paysum_1 ON paysum_1.accrual_pay_id = accrual_pays_1.id
LEFT JOIN (SELECT accrual_pay_id, SUM(sum) as sum FROM payment_details GROUP BY accrual_pay_id) as paysum_2 ON paysum_2.accrual_pay_id = accrual_pays_2.id
LEFT JOIN (SELECT accrual_pay_id, SUM(sum) as sum FROM payment_details GROUP BY accrual_pay_id) as paysum_3 ON paysum_3.accrual_pay_id = accrual_pays_3.id

WHERE meters.disabled = 'FALSE' 
AND (@onlyAccruals = 1 AND 
reading.lastpayid_1 is not null
AND reading.lastpayid_2 is not null
AND reading.lastpayid_3 is not null)
OR (@onlyAccruals = 0)
AND (service_providers.id IN (@providers) OR @providers = -1)
GROUP BY service_provider_name, name
ORDER BY service_provider_name, name, meter_id</CommandText>
