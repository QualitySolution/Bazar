  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>use bazar;
SELECT service_providers.name as service_provider_name, lessees.name, active_contracts.number as contract, place_types.name as place_type, meters.place_no, 
meter_types.name type, meter_types.reading_ratio, meter_tariffs.name as tariff, 
 reading.*, accrual_pays.count, accrual_pays.price, IFNULL(paysum.sum, 0) as paid, IFNULL(reading.lastpayid, -1) as lastpayid0
 FROM meters 
LEFT JOIN (
SELECT  selectedmeters.*, 
(SELECT accrual_pay_id 
	FROM meter_reading 
    WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = meter_month AND YEAR(meter_reading.date) = @year  
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastpayid, 
(SELECT date
	FROM meter_reading 
	WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = meter_month AND YEAR(meter_reading.date) = @year  
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastdate, 
(SELECT value 
	FROM meter_reading 
	WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND MONTH(meter_reading.date) = meter_month  AND YEAR(meter_reading.date) = @year  
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as lastvalue, 
(SELECT value
	FROM meter_reading 
	WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id
	AND meter_reading.date &lt; (SELECT date
								FROM meter_reading 
								WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
								AND MONTH(meter_reading.date) = meter_month AND YEAR(meter_reading.date) = @year  
								ORDER BY meter_reading.date DESC, value DESC LIMIT 1) 
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as prevalue,
(SELECT date
	FROM meter_reading 
	WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
	AND meter_reading.date &lt; (SELECT date
								FROM meter_reading 
								WHERE selectedmeters.meter_id = meter_reading.meter_id AND selectedmeters.meter_tariff_id = meter_reading.meter_tariff_id 
								AND MONTH(meter_reading.date) = meter_month AND YEAR(meter_reading.date) = @year  
								ORDER BY meter_reading.date DESC, value DESC LIMIT 1)
	ORDER BY meter_reading.date DESC, value DESC LIMIT 1) as prevdate  

FROM ( SELECT meter_month.mon as meter_month, meters.id as meter_id,meter_tariffs.id as meter_tariff_id FROM meters JOIN meter_tariffs on meters.meter_type_id=meter_tariffs.meter_type_id
LEFT JOIN (SELECT * FROM (SELECT 1 as mon UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 UNION SELECT 11 UNION SELECT 12) t
WHERE t.mon IN (@month)) meter_month ON 1 
) as selectedmeters
) as reading ON meters.id = reading.meter_id 
LEFT JOIN place_types ON place_types.id = meters.place_type_id
LEFT JOIN meter_types ON meters.meter_type_id = meter_types.id
LEFT JOIN meter_tariffs ON meter_tariffs.id = reading.meter_tariff_id
LEFT JOIN active_contracts ON meters.place_type_id = active_contracts.place_type_id AND meters.place_no = active_contracts.place_no
LEFT JOIN lessees ON active_contracts.lessee_id = lessees.id
LEFT JOIN accrual_pays ON accrual_pays.id = reading.lastpayid
LEFT JOIN services on services.id = accrual_pays.service_id
LEFT JOIN service_providers on service_providers.id = services.service_provider_id
LEFT JOIN (SELECT accrual_pay_id, SUM(sum) as sum FROM payment_details GROUP BY accrual_pay_id) as paysum 
ON paysum.accrual_pay_id = accrual_pays.id
WHERE meters.disabled = 'FALSE' 
AND (-1 IN (@types) OR meters.meter_type_id IN (@types))
AND (-1 IN (@providers) OR service_providers.id IN (@providers))
GROUP BY service_provider_name, name, meter_month
ORDER BY service_provider_name, name, meter_id</CommandText>
