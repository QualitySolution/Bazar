  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT accrual.id as id, month, year, contracts.number as contract_no, contracts.lessee_id as lessee_id, contracts.pay_day, 
 lessees.name as lessee, services.name as service, IFNULL(sumtable.sum, 0) as sum, IFNULL(paidtable.sum, 0) as paidsum,
 (IFNULL(sumtable.sum, 0) - IFNULL(paidtable.sum, 0)) as debt 
FROM accrual 
LEFT JOIN contracts ON contracts.id = contract_id 
LEFT JOIN lessees ON contracts.lessee_id = lessees.id 
LEFT JOIN (
SELECT accrual_id, id as pay_id, service_id, (count * price) as sum FROM accrual_pays WHERE service_id IN (@services) AND cash_id = @cash) as sumtable 
ON sumtable.accrual_id = accrual.id 
LEFT JOIN (
SELECT accrual_pay_id, SUM(sum) as sum FROM payment_details GROUP BY accrual_pay_id) as paidtable 
ON paidtable.accrual_pay_id =  sumtable.pay_id
LEFT JOIN services ON services.id = sumtable.service_id 
LEFT JOIN (
SELECT accrual.id, (IFNULL(sumtable.sum,0) - IFNULL(paidtable.sum, 0)) as debt FROM accrual
LEFT JOIN (SELECT accrual_id, SUM(count * price) as sum FROM accrual_pays GROUP BY accrual_id) as sumtable 
ON sumtable.accrual_id = accrual.id 
LEFT JOIN (SELECT accrual_id, SUM(sum) as sum FROM credit_slips GROUP BY accrual_id) as paidtable
ON paidtable.accrual_id = accrual.id) as accrual_debt 
ON accrual_debt.id = accrual.id
WHERE sumtable.pay_id IS NOT NULL AND
(accrual.year &lt; @year OR (accrual.year = @year AND accrual.month &lt; @month) 
OR (accrual.year = @year AND accrual.month = @month AND IFNULL(contracts.pay_day, 0) &lt; @day)) 
AND (IFNULL(sumtable.sum, 0) - IFNULL(paidtable.sum, 0)) &gt; 0 AND accrual_debt.debt &gt; 0</CommandText>
        <QueryParameters>
          <QueryParameter Name="month">
            <Value>=Parameters!Month</Value>
          </QueryParameter>
          <QueryParameter Name="year">
            <Value>=Parameters!Year</Value>
          </QueryParameter>
          <QueryParameter Name="day">
            <Value>=Parameters!Day</Value>
          </QueryParameter>
          <QueryParameter Name="cash">
            <Value>=Parameters!Cash</Value>
          </QueryParameter>
          <QueryParameter Name="services">
            <Value>=Parameters!Services</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
