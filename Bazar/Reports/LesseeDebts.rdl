  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT
    accrual.id AS id,
    accrual.month,
    accrual.year,
    accrual.date,
    contracts.number AS contract_no,
    contracts.lessee_id AS lessee_id,
    contracts.pay_day,
    lessees.name AS lessee,
    IFNULL(sumtable.sum, 0) AS sum,
    IFNULL(paidtable.sum, 0) AS paidsum,
    (
        IFNULL(sumtable.sum, 0) - IFNULL(paidtable.sum, 0)
    ) AS debt
FROM
    accrual
LEFT JOIN contracts ON contracts.id = contract_id
LEFT JOIN lessees ON contracts.lessee_id = lessees.id
LEFT JOIN(
    SELECT accrual_id,
        SUM(accrual_pays.count * price) AS sum
    FROM
        accrual_pays
    WHERE
        (@cash = 0 OR cash_id = @cash)
    GROUP BY
        accrual_id
) AS sumtable
ON
    sumtable.accrual_id = accrual.id
LEFT JOIN(
    SELECT accrual_id,
        SUM(sum) AS sum
    FROM
        credit_slips
    WHERE
        (@cash = 0 OR cash_id = @cash)
    GROUP BY
        accrual_id
) AS paidtable
ON
    paidtable.accrual_id = accrual.id
WHERE
    (accrual.year &lt; @year OR (accrual.year = @year AND accrual.month &lt; @month ) OR ( accrual.year = @year AND accrual.month = @month AND IFNULL(contracts.pay_day, 0) &lt; @day)
    ) AND(
        IFNULL(sumtable.sum, 0) - IFNULL(paidtable.sum, 0)
    ) &gt; 0 AND( @org = 0 OR contracts.org_id = @org)
    AND (@lessee = -1 OR contracts.lessee_id = @lessee)
    AND ((@month_from = -1 AND @year_from = -1) OR (accrual.month + accrual.year *12 &gt;= @month_from + @year_from * 12))
    AND ((@month_to = -1 AND @year_to = -1) OR (accrual.month + accrual.year *12 &lt;= @month_to + @year_to * 12))    
    ORDER BY lessees.name, accrual.year, accrual.month, accrual.date</CommandText>
        <QueryParameters>
          <QueryParameter Name="month">
            <Value>=Parameters!Month</Value>
          </QueryParameter>
          <QueryParameter Name="year">
            <Value>=Parameters!Year</Value>
          </QueryParameter>
          <QueryParameter Name="day">
            <Value>=Parameters!Day</Value>
          </QueryParameter>
          <QueryParameter Name="cash">
            <Value>=Parameters!Cash</Value>
          </QueryParameter>
          <QueryParameter Name="org">
            <Value>=Parameters!Org</Value>
          </QueryParameter>
          <QueryParameter Name="lessee">
            <Value>={?Lessee}</Value>
          </QueryParameter>
          <QueryParameter Name="month_from">
            <Value>={?MonthFrom}</Value>
          </QueryParameter>
          <QueryParameter Name="month_to">
            <Value>={?MonthTo}</Value>
          </QueryParameter>
          <QueryParameter Name="year_from">
            <Value>={?YearFrom}</Value>
          </QueryParameter>
          <QueryParameter Name="year_to">
            <Value>={?YearTo}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
