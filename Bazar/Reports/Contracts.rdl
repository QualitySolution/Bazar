  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT contracts.*, lessees.name as lessee, organizations.name as organization, IFNULL(contracts.cancel_date, contracts.end_date) as end, 
(SELECT GROUP_CONCAT(DISTINCT CONCAT(place_types.name, "-", places.place_no) SEPARATOR ", ") 
	FROM contract_pays 
 	INNER JOIN places ON places.id = contract_pays.place_id
	INNER JOIN place_types ON place_types.id = places.type_id
 	WHERE contract_pays.contract_id = contracts.id
) as contract_places
FROM contracts
LEFT JOIN lessees ON contracts.lessee_id = lessees.id 
LEFT JOIN organizations ON contracts.org_id = organizations.id
WHERE ((contracts.cancel_date IS NULL AND CURDATE() BETWEEN contracts.start_date AND contracts.end_date) 
OR (contracts.cancel_date IS NOT NULL AND CURDATE() BETWEEN contracts.start_date AND contracts.cancel_date))
AND (contracts.org_id = @org_id OR @org_id = 0)
ORDER BY contracts.number;</CommandText>
        <QueryParameters>
          <QueryParameter Name="org_id">
            <Value>=Parameters!org_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
